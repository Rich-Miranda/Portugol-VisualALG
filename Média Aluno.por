programa {
  funcao inicio() {

    caracter nome
    real n1,n2,n3,total

    escreva("Informe seu nome: ")
    leia(nome)

    escreva("Informe um valor para n1: ")
    leia(n1)

    escreva("Informe um valor para n2: ")
    leia(n2)

    escreva("Informe um valor para n3: ")
    leia(n3)


    total = (n1+n2+n3)/3

    escreva("O ALUNO: "+ nome + "\tTIROU A NOTA: "+ total)



  }
}
